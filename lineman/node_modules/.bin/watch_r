#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../watch_r-structr-lock/bin/watch_r" "$@"
  ret=$?
else 
  node  "$basedir/../watch_r-structr-lock/bin/watch_r" "$@"
  ret=$?
fi
exit $ret
